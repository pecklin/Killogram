import java.awt.*;
import javax.swing.*;

public class Main extends JFrame{
	
	Main(){
		
		ImageIcon img1 = new ImageIcon("배달이미지.jpg");
		
		JPanel panel = new JPanel();
		
		JButton b = new JButton(img1);
		b.setBorderPainted(false);
		b.setBounds(470, 750, 250, 80);
		
		panel.add(b);
		add(panel);
		setTitle("JButton EX");
		setSize(1200, 1000);
		setLayout(null);
		setVisible(true);
	}
	
	public static void main(String[] args) {
		new Main();
	}
}


import java.awt.*;
import javax.swing.*;

public class Main extends JFrame{
	
	Main(){
		JButton b = new JButton("선택");
		
		ImageIcon img = new ImageIcon("배달이미지.jpg");
		
		JPanel panel = new JPanel();
		
		
		
		b.setBounds(470, 750, 250, 80);
		
		add(b);
		setTitle("JButton EX");
		setSize(1200, 1000);
		setLayout(null);
		setVisible(true);
	}
	
	public static void main(String[] args) {
		new Main();
	}
}


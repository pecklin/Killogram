import javax.swing.JFrame;

public class login extends JFrame{
	login() {
		JPanel panel = new JPanel();
		
		setVisible(true);
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setSize(600, 400);
		setLocationRelativeTo(null);
		setResizable(false);
	}
	public static void main(String args[]) {
		new login();
	}
}
